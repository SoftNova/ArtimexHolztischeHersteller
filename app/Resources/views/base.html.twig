<!DOCTYPE html>
<html>
    {% block head %}
        <head>
            <meta charset="UTF-8" />
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="viewport" content="width=device-width, initial-scale=1">
            <title>{% block title %}Hersteller Massivholztische by Artimex Sport{% endblock %}</title>
            <link rel="icon" type="image/x-icon" href="{{ asset ('bundles/app/img/favicon.ico')}}" />
            {% block stylesheets %}
                <link rel="stylesheet" href="{{ asset ('bundles/app/css/style.css')}}">
                <link rel="stylesheet" href="{{ asset ('bundles/app/css/owl.carousel.css') }}">
                <link rel="stylesheet" href="{{ asset ('bundles/app/css/responsive.css') }}">
                <link rel="stylesheet" href="{{ asset ('bundles/app/css/lightbox.css') }}">
                <link rel="stylesheet" href="{{ asset ('bundles/app/css/tooltipster.css') }}">
                <link rel="stylesheet" href="{{ asset ('bundles/app/css/progress.bar.css') }}">

                <!-- Google Fonts -->
                <link href='http://fonts.googleapis.com/css?family=Titillium+Web:400,200,300,700,600' rel='stylesheet' type='text/css'>
                <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:400,700,300' rel='stylesheet' type='text/css'>
                <link href='http://fonts.googleapis.com/css?family=Raleway:400,100' rel='stylesheet' type='text/css'>

                <!-- Bootstrap -->
                <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">

                <!-- Font Awesome -->
                <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css">

            {% endblock %}

        </head>
    {% endblock %}
    <body>
        {% block submenu %}{% endblock %}
        {% block content %}{% endblock %}
        {% block footer %}
            {% block javascripts %}
                <script src="https://code.jquery.com/jquery.min.js"></script>
                <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.1/jquery-ui.min.js"></script>
                <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
                <script src="{{ asset('bundles/app/js/activeNav.js') }}" type="text/javascript"></script>
                <script type="text/javascript" src="{{ asset('bundles/app/js/jquery.tooltipster.min.js') }}"></script>
                <script src="{{ asset ('bundles/app/js/owl.carousel.min.js') }}" type="text/javascript"></script>
                <script src="{{ asset ('bundles/app/js/jquery.sticky.js') }}" type="text/javascript"></script>

                <script src="{{ asset ('bundles/app/js/lightbox.js') }}" type="text/javascript"></script>
                <script src="{{ asset ('bundles/app/js/jquery.easing.1.3.min.js')}}"></script>
                <script src="{{ asset ('bundles/app/js/main.js') }}"></script>

                <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
                <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>

                <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
                <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>

            {% endblock %}
        {% endblock %}
    </body>
</html>
